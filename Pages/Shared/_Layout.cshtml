<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>SLED </title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap-icons@1.3.0/font/bootstrap-icons.css">
    <link rel="stylesheet" href="https://kit.fontawesome.com/619fe32a9c.css" crossorigin="anonymous">    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.js"></script>
</head>
<body >


<div class="container-fluid bg-white mb-5" style="border-bottom: 3px solid #003e7a;">
    <nav class="navbar justify-content-center navbar-expand-lg ">
        <a class="navbar-brand d-none d-sm-none d-md-block d-lg-block d-xl-block " href="/">
            <img id="logo" src="/Images/waukesha_logo.png" width="250" height="auto" alt="Waukesha County Technical College"/>
        </a>

        <a class="navbar-brand d-block d-sm-block d-md-none d-lg-none d-xl-none " href="/">
            <img id="logo" src="/Images/wctc-logo-sm.jpg" width="55" height="55" alt="Waukesha County Technical College"/>
        </a>



        <div class=" ms-auto " style="z-index: 9">
            <button class=" btn btn-outline-primary  " type="button" id="dropdownMenuButton1" data-bs-toggle="dropdown" aria-expanded="false">
                Sign-out
            </button>

        </div>

    </nav>
</div>
<div class="alert alert-success alert-dismissible fade hide" role="alert">
    A simple success alert—check it out!
    <button type="button" class="close" data-dismiss="alert" aria-label="Close">
        <span aria-hidden="true">&times;</span>
    </button>
</div>

<div class="container text-center">
    <h3>Welcome Username</h3>
</div>

<div class="container">



    <div class="row ">
        <ul class="nav nav-tabs  justify-content-start ">
            @*Needs to be manually updated for each new controller*@

            <li class="nav-item h3 ">
                <a class="nav-link active align-bottom h5" href="~/Admin/AdminDashboard">Admin Dashboard</a>
            </li>


            <li class="nav-item h3 ">
                <a class="nav-link align-bottom h5" href="~/InstructorAssessments/Dashboard">Faculty Dashboard</a>
            </li>

        </ul>
    </div>




    <div class="bg-white mb-4" id="MainBody">
        @RenderBody()
    </div>
<footer class="footer mt-4 py-3 bg-light bottom">
    <div class="container row">
        <div class="col-4">

                <img id="logo" src="/Images/waukesha_logo.png" width="125" height="auto" alt="Waukesha County Technical College" />

        </div>
        <div class="col-4 text-center">
            <a href="https://www.wctc.edu/">WCTC</a>
        </div>
        <div class="col-4">
            <a href="https://www.wctc.edu/">Version: 1.0</a>
        </div>
    </div>
</footer>
</div>





@RenderSection("scripts", required: false)
</body>





</html>